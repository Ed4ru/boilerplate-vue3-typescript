{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/typescript/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "vue/script-setup-uses-vars": "error",
    "vue/multi-word-component-names": 0,
    "vue/no-setup-props-destructure": 0
  },
  "overrides": []
}
